{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/country/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/detect-locale/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/about-us/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/contacto/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/news/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/nosotros/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/noticias/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/productos/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/products/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/recetas/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/recipes/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}